<Window x:Class="CarControl.Auto_Delete"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CarControl"
        mc:Ignorable="d"
        Title="Auto_Delete" Height="450" Width="800">
    <Grid>
        <TextBox x:Name="DeleteText" Margin="100"></TextBox>
        <Button x:Name="Del_Btn" VerticalAlignment="Bottom" HorizontalAlignment="Center" Margin="20" Content="Удалить" Click="Del_Dtn_Click" ></Button>
    </Grid>
</Window>
